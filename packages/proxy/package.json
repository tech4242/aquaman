{
  "name": "aquaman-proxy",
  "version": "0.7.1",
  "description": "Credential isolation proxy daemon for OpenClaw",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "aquaman": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./daemon": {
      "types": "./dist/daemon.d.ts",
      "import": "./dist/daemon.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli/index.js",
    "dev": "tsx watch src/cli/index.ts"
  },
  "keywords": [
    "aquaman",
    "openclaw",
    "openclaw-plugin",
    "security",
    "credentials",
    "proxy"
  ],
  "author": "tech4242",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "yaml": "^2.3.4",
    "argon2": "^0.44.0",
    "kdbxweb": "^2.1.1"
  },
  "optionalDependencies": {
    "keytar": "^7.9.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  },
  "files": ["dist", "LICENSE", "README.md"],
  "repository": {
    "type": "git",
    "url": "https://github.com/tech4242/aquaman.git",
    "directory": "packages/proxy"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://github.com/tech4242/aquaman/tree/main/packages/proxy#readme",
  "bugs": {
    "url": "https://github.com/tech4242/aquaman/issues"
  }
}
