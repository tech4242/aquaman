{
  "name": "aquaman-core",
  "version": "0.1.0",
  "description": "Core credential storage, audit logging, and utilities for aquaman",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./credentials": {
      "types": "./dist/credentials/index.d.ts",
      "import": "./dist/credentials/index.js"
    },
    "./audit": {
      "types": "./dist/audit/index.d.ts",
      "import": "./dist/audit/index.js"
    },
    "./utils": {
      "types": "./dist/utils/index.d.ts",
      "import": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "aquaman",
    "credentials",
    "audit",
    "keychain",
    "vault",
    "1password"
  ],
  "author": "tech4242",
  "license": "MIT",
  "dependencies": {
    "keytar": "^7.9.0",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "files": ["dist", "LICENSE", "README.md"],
  "repository": {
    "type": "git",
    "url": "https://github.com/tech4242/aquaman.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "homepage": "https://github.com/tech4242/aquaman/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/tech4242/aquaman/issues"
  }
}
