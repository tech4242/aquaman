{
  "name": "aquaman",
  "version": "0.1.0",
  "private": true,
  "description": "Credential isolation for OpenClaw - keeps API keys outside the agent process",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "build:core": "npm run build -w aquaman-core",
    "build:proxy": "npm run build -w aquaman-proxy",
    "build:openclaw": "npm run build -w aquaman-plugin",
    "clean": "npm run clean --workspaces && rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "oxlint src test packages --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern coverage",
    "lint:fix": "oxlint src test packages --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern coverage --fix",
    "typecheck": "tsc -b packages/core packages/proxy packages/openclaw",
    "start": "node packages/proxy/dist/cli/index.js",
    "dev": "tsx watch packages/proxy/src/cli/index.ts"
  },
  "keywords": [
    "openclaw",
    "security",
    "audit",
    "credentials",
    "ai-agent",
    "keychain",
    "vault",
    "1password"
  ],
  "author": "tech4242",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^1.2.0",
    "oxlint": "^0.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
