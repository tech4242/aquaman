{
  "name": "aquaman-clawed",
  "version": "0.1.0",
  "description": "Security wrapper for OpenClaw - audit logging, guardrails, and credential isolation",
  "type": "module",
  "main": "dist/cli/index.js",
  "bin": {
    "aquaman": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --config vitest.unit.config.ts",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "lint": "oxlint src test --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern coverage",
    "lint:fix": "oxlint src test --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern coverage --fix",
    "typecheck": "tsc --noEmit",
    "start": "node dist/cli/index.js",
    "dev": "tsx watch src/cli/index.ts"
  },
  "keywords": [
    "openclaw",
    "security",
    "audit",
    "credentials",
    "ai-agent"
  ],
  "author": "tech4242",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/ws": "^8.5.10",
    "@vitest/coverage-v8": "^1.2.0",
    "oxlint": "^0.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "keytar": "^7.9.0",
    "minimatch": "^9.0.3",
    "ws": "^8.16.0",
    "yaml": "^2.3.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
