{
  "agents": {
    "defaults": {
      "sandbox": {
        "mode": "all",
        "scope": "session",
        "workspaceAccess": "rw",
        "docker": {
          "network": "aquaman-frontend",
          "env": {
            "ANTHROPIC_BASE_URL": "http://aquaman:8081/anthropic",
            "OPENAI_BASE_URL": "http://aquaman:8081/openai"
          },
          "readOnlyRoot": true,
          "capDrop": ["ALL"],
          "user": "1000:1000"
        }
      }
    }
  },
  "plugins": {
    "entries": {
      "aquaman-plugin": {
        "enabled": true,
        "config": {
          "mode": "proxy",
          "backend": "encrypted-file",
          "services": ["anthropic", "openai"],
          "proxyPort": 8081
        }
      }
    }
  }
}
